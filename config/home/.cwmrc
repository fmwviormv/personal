# my extensions

bind-key	S4-Delete	"/bin/sh -c 'setxkbmap us colemak && xlock -mode maze -speed 40'"
bind-key	S4-m		"/usr/X11R6/bin/xterm -fullscreen -fa 'Inconsolata:size=16' -title tmux /usr/bin/tmux"
bind-key	S4-n		"/usr/X11R6/bin/xterm -fullscreen -fa 'Inconsolata:size=16' -title ksh /bin/ksh"
bind-key	4-space		"/bin/sh -c \"if setxkbmap -query | grep '^layout.*us$' > /dev/null; then setxkbmap ir; else setxkbmap us colemak; fi\""
bind-key	S4-space	"/bin/sh -c \"if setxkbmap -query | grep '^layout.*us$' > /dev/null; then setxkbmap ir; else setxkbmap us; fi\""
bind-key	C4-equal	window-vtile
bind-key	CS4-equal	window-htile

sticky yes
